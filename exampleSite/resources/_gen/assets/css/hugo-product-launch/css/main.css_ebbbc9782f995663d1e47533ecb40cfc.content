    padding-right: 1.25rem;
  }

  .md\:pb-24 {
    padding-bottom: 6rem;
  }

  .md\:text-center {
    text-align: center;
  }

  .md\:gap-10 {
    grid-gap: 2.5rem;
    gap: 2.5rem;
  }

  .md\:grid-cols-2 {
    grid-template-columns: repeat(2, minmax(0, 1fr));
