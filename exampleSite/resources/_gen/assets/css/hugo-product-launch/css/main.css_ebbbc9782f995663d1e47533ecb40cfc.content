@media (min-width: 640px) {

  .sm\:text-lg {
    font-size: 1.125rem;
  }

  .sm\:text-4xl {
    font-size: 2.25rem;
  }

  .sm\:max-w-2xl {
    max-width: 42rem;
  }
}

@media (min-width: 768px) {

  .md\:block {
    display: block;
  }

  .md\:justify-center {
    justify-content: center;
  }

  .md\:text-lg {
    font-size: 1.125rem;
  }

  .md\:text-xl {
    font-size: 1.25rem;
  }

  .md\:text-5xl {
    font-size: 3rem;
  }

  .md\:py-4 {
    padding-top: 1rem;
    padding-bottom: 1rem;
  }

  .md\:px-5 {
